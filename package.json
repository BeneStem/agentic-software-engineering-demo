{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && run-s build-client--production build-server--production build-tool-client--production buildSsrServer",
    "dev": "APP=produktliste vue-cli-service serve --open",
    "test": "npm run lint && npm run unitTest",
    "unitTest": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "lint": "stylelint 'src/main/frontend/**/*.vue' && vue-cli-service lint 'src/main/frontend/app/**' && eslint 'src/main/frontend/ssrServer/**'",
    "dependency-updates": "ncu -e 2",
    "build-client--production": "APP=produktliste vue-cli-service build --mode=production",
    "build-server--production": "SSR=true APP=produktliste vue-cli-service build --mode=production",
    "build-tool-client--production": "APP=tool vue-cli-service build --mode=production",
    "profile": "OPEN_ANALYZER=true npm run build",
    "buildSsrServer": "tsc --project src/main/frontend/ssrServer/tsconfig.ssrServer.json",
    "buildSsrServerDev": "npm run buildSsrServer -- --watch",
    "server": "NODE_ENV=production node dist/ssrServer/server.js",
    "serverDev": "nodemon dist/ssrServer/server.js",
    "vueDev": "vue-cli-service serve"
  },
  "dependencies": {
    "@fastify/env": "4.0.0",
    "@fastify/static": "6.4.0",
    "@google-cloud/opentelemetry-cloud-monitoring-exporter": "0.14.0",
    "@opentelemetry/api": "1.0.4",
    "@opentelemetry/sdk-metrics-base": "0.27.0",
    "@vue/server-renderer": "3.2.37",
    "@vueuse/core": "8.6.0",
    "axios": "0.27.2",
    "core-js": "3.23.2",
    "date-fns": "2.28.0",
    "dotenv": "16.0.1",
    "events": "3.3.0",
    "fastify": "4.2.0",
    "fastify-metrics": "9.0.0",
    "pino": "8.0.0",
    "regenerator-runtime": "0.13.9",
    "serialize-javascript": "6.0.0",
    "vue": "3.2.37",
    "vue-router": "4.0.16",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@types/jest": "28.1.3",
    "@types/node": "17.0.16",
    "@types/serialize-javascript": "5.0.2",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@vue/cli-plugin-babel": "5.0.4",
    "@vue/cli-plugin-eslint": "5.0.4",
    "@vue/cli-plugin-router": "5.0.4",
    "@vue/cli-plugin-typescript": "5.0.4",
    "@vue/cli-service": "5.0.4",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.0.1",
    "@vue/vue3-jest": "28.0.0",
    "eslint": "8.18.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "fork-ts-checker-webpack-plugin": "7.2.11",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-junit": "13.2.0",
    "nodemon": "2.0.16",
    "npm-check-updates": "13.1.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sass": "1.52.3",
    "sass-loader": "13.0.0",
    "sass-resources-loader": "2.2.5",
    "stylelint": "14.9.1",
    "stylelint-config-recommended-scss": "6.0.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-scss": "4.2.0",
    "ts-jest": "28.0.5",
    "typescript": "4.7.3",
    "vue-bundle-renderer": "0.3.9",
    "vue-cli-plugin-webpack-bundle-analyzer": "4.0.0",
    "vue-loader": "17.0.0",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-node-externals": "3.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  }
}
